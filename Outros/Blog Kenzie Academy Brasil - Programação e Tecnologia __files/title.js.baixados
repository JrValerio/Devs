const latestTitle=Array.from(document.getElementsByClassName("latest__title"));latestTitle.forEach((title)=>{const titleLenght=title.innerText.length;if(window.innerWidth<1024&&titleLenght>80){title.innerText=title.innerText.slice(0,80)+"..."}else if(window.innerWidth>=1024&&window.innerWidth<1285&&titleLenght>72){title.innerText=title.innerText.slice(0,72)+"..."}else if(window.innerWidth>=1285&&titleLenght>85){title.innerText=title.innerText.slice(0,85)+"..."}})